[ziina]
# Your Ziina API bearer token (replace with the real token in `.streamlit/secrets.toml`)
access_token = "REPLACE_WITH_ZIINA_ACCESS_TOKEN"

# Your app base URL where Ziina will redirect after payment (no trailing slash)
app_base_url = "https://snow-liwa.streamlit.app"

# Whether to create test payment intents (true/false)
test_mode = false